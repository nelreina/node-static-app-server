{
  "appenders": {
    "console": {
      "type": "console"
    },
    "file": {
      "type": "file",
      "filename": "app.log"
    },
    "redis": {
      "type": "redis",
      "channel": "logs"
    }
  },
  "categories": {
    "default": {
      "appenders": ["console"],
      "level": "error"
    },
    "app": {
      "appenders": ["console", "file"],
      "level": "debug"
    }
  }
}
